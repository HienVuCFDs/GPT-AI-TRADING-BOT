# Environment Variables Template
# Copy this to .env and update the values

# Django Settings
DJANGO_SECRET_KEY=your-super-secret-key-change-this-in-production
DJANGO_SETTINGS_MODULE=license_server.settings_production

# Database Settings
DB_NAME=license_db
DB_USER=license_user
DB_PASSWORD=your-strong-database-password
DB_HOST=db
DB_PORT=5432

# Domain (for ALLOWED_HOSTS)
DOMAIN=license.yourdomain.com

# Admin User (created on first deploy)
DJANGO_SUPERUSER_USERNAME=admin
DJANGO_SUPERUSER_EMAIL=admin@yourdomain.com
DJANGO_SUPERUSER_PASSWORD=your-admin-password
